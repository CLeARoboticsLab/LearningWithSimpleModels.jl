<launch>
    <!-- gazebo motion capture simulation -->
    <node pkg="ros_sockets" type="gazebo_tracker" name="gazebo_tracker"  output="screen">
        <param name="tracker_name" value="jetracer" type="str" />
        <param name="model_name" value="turtlebot3_burger" type="str" />
        <param name="update_frequency" value="2.0" type="double" />
    </node>

    <!-- Controller -->
    <node pkg="lwsm_jetracer" type="controller" name="controller"  output="screen" />
</launch>